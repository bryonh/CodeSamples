<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="commandFilters" type="System.Configuration.NameValueSectionHandler" />
  </configSections>
  <startup useLegacyV2RuntimeActivationPolicy="true"><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup>
  <appSettings>
    <add key="XECaptureServer" value="(local)\sql2014"/>
    <add key="XEReplayServer" value="(local)\sql2016"/>
    <add key="LogDirectory" value="C:\Temp\"/>
  </appSettings>
  <commandFilters>
    <add key="Filter1" value="exec sp_reset_connection"/>
    <add key="Filter3" value="sp_unprepare"/>
  </commandFilters>
</configuration>
